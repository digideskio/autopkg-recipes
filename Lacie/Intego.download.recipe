<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads Intego Backup Assistand.</string>
    <key>Identifier</key>
    <string>com.github.uib.autopkg-recipes.download.Intego</string>
    <key>Input</key>
    <dict>
    	<key>DOWNLOAD_URL</key>
    	<string>http://www.integodownload.com/mac/X/LaCie/IntegoBackupAssistant.dmg</string>
        <key>NAME</key>
        <string>Intego Backup Assistant</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%DOWNLOAD_URL%</string>
                <key>filename</key>
                <string>%NAME%.dmg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>